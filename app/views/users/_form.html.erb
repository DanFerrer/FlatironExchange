<%= form_for(@user) do |f| %>
  <div class="field">
    <br><%= f.label :name, "Name" %>
    <br><%= f.text_field :name %></br>
    <br><%= f.label :email, "Email" %>
    <br><%= f.text_field :email %><br>
    <br><%= f.label :title, "Title" %>
		<%= f.select :title, @title.collect {|t| [t]}, {} %>	
  </div><br>

  <div class="action">
    <%= f.submit "Submit", class: "btn btn-primary" %>
  </div><br>
<% end %><br />