<h1 class="text-center">Community Activity</h1>
<table class="table table-responsive table-hover">
  <tbody>
    <% @activities.each do |activity| %>
      <tr>
        <td><img src="<%= activity.user.image_url%>" class="img-circle" height="20" align="left">
          <%= ActivityPresenter.new(activity, self).render_activity %></td>
      </tr>
    <% end %>
  </tbody>
</table>