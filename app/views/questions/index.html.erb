<p id="notice"><%= notice %></p>

<h1>Archive</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.title %></td>
        <td><%= question.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>